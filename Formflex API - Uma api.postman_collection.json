						"body": {
							"mode": "raw",
							"raw": "{\n    \"form\": 6,\n    \"title\": \"Qual a sua cor favorita?\",\n    \"question_type\": \"choice\",\n    \"choices\": {\n        \"azul\": {\n            \"title\": \"Você pintaria sua parede de azul?\",\n            \"question_type\": \"text\",\n            \"question\": {\n                \"title\": \"O que você gostaria de fazer a seguir?\",\n                \"question_type\": \"text\"\n            }\n        },\n        \"amarelo\": {\n            \"title\": \"Qual tom de amarelo\",\n            \"question_type\": \"choice\",\n            \"choices\": {\n                \"claro\": {\n                    \"title\": \"Digite seu e-mail para mais informações\",\n                    \"question_type\": \"text\"\n                },\n                \"escuro\": {\n                    \"title\": \"Digite seu e-mail para mais informações\",\n                    \"question_type\": \"text\"\n                }\n            }\n        }\n    }\n}",
